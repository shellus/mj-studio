{
  "name": "mj-studio",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "pnpm build:docs && nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "docs:dev": "vitepress dev docs --port 4173 --host 0.0.0.0",
    "docs:build": "vitepress build docs",
    "build:docs": "vitepress build docs",
    "test": "vitest run --exclude tests/api-integration.test.ts --exclude tests/mcp-integration.test.ts",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "test:shared": "vitest run tests/shared.test.ts",
    "test:api": "vitest run tests/api-integration.test.ts"
  },
  "dependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.81",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@nuxt/ui": "^4.2.1",
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.3",
    "@vue-flow/core": "^1.48.1",
    "@vue-flow/minimap": "^1.5.4",
    "@vue-flow/node-resizer": "^1.5.0",
    "@vueuse/core": "^14.1.0",
    "drizzle-orm": "^0.45.1",
    "jose": "^6.1.3",
    "marked": "^17.0.1",
    "nuxt": "^4.2.2",
    "shiki": "^3.20.0",
    "tailwindcss": "^4.1.18",
    "vuedraggable": "^4.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.21.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/uuid": "^11.0.0",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "mermaid": "^11.12.2",
    "msw": "^2.12.7",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vitest": "^4.0.15"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  }
}
